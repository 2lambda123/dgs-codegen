type Query {
    result: Result!
    find(filter: Filter!): Result!
    interface(params: InterfaceParams): String
}

type Result {
    isSuccessful: Boolean
    result: String
}

input Filter {
    mandatoryString: String!
    optionalString: String
    mandatoryNumber: Int!
    optionalNumber: Int
}


input InterfaceParams {
    class: String
}